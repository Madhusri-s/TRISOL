# EcoInnovators PV Detection Environment Variables
# Copy this file to .env and fill in your actual values

# Roboflow API Configuration
ROBOFLOW_API_KEY=your_roboflow_api_key_here
ROBOFLOW_WORKSPACE=alfred-weber-institute-of-economics
ROBOFLOW_PROJECT=custom-workflow-object-detection-tgnqc
ROBOFLOW_VERSION=8

# Pipeline Configuration
BASE_DIR=/app/data
OUTPUT_DIR=/app/outputs
LOG_LEVEL=INFO

# Model Configuration
MODEL_ID=custom-workflow-object-detection-tgnqc/8
CONFIDENCE_THRESHOLD=0.5
NMS_THRESHOLD=0.4

# Docker Configuration
CONTAINER_NAME=eco-pv-pipeline
HOST_PORT=8080
JUPYTER_PORT=8888